<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/btnLimpiar"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/backgroud3"
    tools:context=".Calcular">

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="44dp"
        android:text="Número:"
        android:textColor="#F8F6F6"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.098"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/etNumero"
        android:layout_width="470dp"
        android:layout_height="47dp"
        android:layout_marginTop="40dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:textColor="#EFEFEF"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.674"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btnRutinaKaprekar"
        android:layout_width="585dp"
        android:layout_height="46dp"
        android:text="Rutina de Kaprekar"
        tools:layout_editor_absoluteX="61dp"
        tools:layout_editor_absoluteY="105dp" />

    <EditText
        android:id="@+id/editTextTextMultiLine2"
        android:layout_width="588dp"
        android:layout_height="129dp"
        android:background="#030303"
        android:ems="10"
        android:gravity="start|top"
        android:inputType="textMultiLine"
        tools:layout_editor_absoluteX="61dp"
        tools:layout_editor_absoluteY="214dp" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Número de Iteraciones: "
        android:textColor="#FBF9F9"
        android:textSize="20sp"
        tools:layout_editor_absoluteX="61dp"
        tools:layout_editor_absoluteY="164dp" />

    <TextView
        android:id="@+id/tvIteracion"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Iteracion"
        android:textColor="#FBF7F7"
        android:textSize="20sp"
        tools:layout_editor_absoluteX="274dp"
        tools:layout_editor_absoluteY="164dp" />

    <Button
        android:id="@+id/button3"
        android:layout_width="145dp"
        android:layout_height="47dp"
        android:text="Limpiar"
        tools:layout_editor_absoluteX="501dp"
        tools:layout_editor_absoluteY="151dp" />

    <Button
        android:id="@+id/btnRegresar"
        android:layout_width="585dp"
        android:layout_height="41dp"
        android:text="Regresar"
        tools:layout_editor_absoluteX="64dp"
        tools:layout_editor_absoluteY="345dp" />
</androidx.constraintlayout.widget.ConstraintLayout>